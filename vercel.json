{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "server/public",
  "functions": {
    "api/donations.ts": {
      "runtime": "@vercel/node"
    },
    "api/donation-stats.ts": {
      "runtime": "@vercel/node"
    },
    "api/impact-metrics.ts": {
      "runtime": "@vercel/node"
    },
    "api/test.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
